                    (
                        base.OnInitalized (e) ;

                        speech = new SpeechSynthesizer () ;
                        speech.SpeakProgress += new EventHandler<SpeakProgressEventArgs>(speech_ScrollBarVisibility= "Auto" TextWrapping= "WrapWithOverflow" LostFocus= "txtText_LostFocus" ></TextBox>
                        }

